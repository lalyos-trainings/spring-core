<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="consoleMessageView" class="com.epam.training.simple.ConsoleMessageView" >
  </bean>
  
  <bean id="staticMessageService" class="com.epam.training.simple.StaticGreetingMessageService"></bean>
  
  <bean id="greetingApp" class="com.epam.training.GreetingApp">
    <property name="messageService" ref="staticMessageService" />
    <property name="messageView" ref="consoleMessageView" />
  </bean>
</beans>
